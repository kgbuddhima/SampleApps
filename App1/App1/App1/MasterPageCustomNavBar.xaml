<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="App1.MasterPageCustomNavBar"
             xmlns:pages="clr-namespace:App1"
             xmlns:local="clr-namespace:App1.Renderers">
    <MasterDetailPage.Master>
        <pages:MasterPageCustomNavBarMaster x:Name="MasterPage" />
  </MasterDetailPage.Master>
  <MasterDetailPage.Detail>
    <local:CustomNavigationPage>
      <x:Arguments>
        <pages:MasterPageCustomNavBarDetail />
      </x:Arguments>
    </local:CustomNavigationPage>
  </MasterDetailPage.Detail>
</MasterDetailPage>